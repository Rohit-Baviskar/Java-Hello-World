pipeline {
  agent any
  tools {
    maven 'MAVEN' 
  }
  stages {
    stage ('Build') {
      steps {
        bat 'mvn clean package'
      }
    }
    stage ('Deploy') {
      steps {
        script {
          deploy adapters: [tomcat9(credentialsId: 'ea184fa8-f58c-4e89-8986-972bd9ce8b00', path: '', url: 'https://localhost:8090')], contextPath: 'pipelinejob', war: '**/*.war'
        }
      }
    }
  }
}
